{
    "manifest_version": 3,
    "name": "Link Content",
    "description": "get me link content",
    "version": "0.0.1",
    "icons": {
        "16": "logo/logo-16.png",
        "48": "logo/logo-48.png",
        "128": "logo/logo-128.png"
    },
    "options_page": "settings/settings.html",
    "action": {
        "default_title": "Chrome Addon v3 Starter",
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "activeTab"    ],
    "host_permissions": [
        "*://*/*"
    ],
    "background": {
        "service_worker": "service-worker.js"
    },
    "web_accessible_resources": [
        {
            "resources": ["button.svg", "button.png"],
            "matches": ["<all_urls>"]
        }
    ],
    "content_scripts": [{
        "js": ["foreground.js"],
        "matches": ["<all_urls>"]
        }]
}
